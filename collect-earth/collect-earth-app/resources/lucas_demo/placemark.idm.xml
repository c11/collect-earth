	<schema>
		<entity id="1" name="plot" ui:tabSet="placemark" ui:tab="placemark">
			<label xml:lang="en">Plot</label>
			<text id="2" name="id" required="true" key="true">
				<label xml:lang="en">Id</label>
			</text>
			<coordinate id="3" name="location">
				<label xml:lang="en">Location</label>
			</coordinate>
			<text id="4" name="operator">
				<label xml:lang="en">Operator</label>
			</text>
			<code id="5" name="land_use_category" list="land_use">
				<label xml:lang="en">Land use</label>
			</code>
			<boolean id="6" name="land_use_category_accuracy" relevant="land_use_category!='noData'">
				<label xml:lang="en">Land use Category interpretation accuracy</label>
			</boolean>

			<code id="9" name="land_use_subdivision" list="land_use"
				parent="land_use_category">
				<label xml:lang="en">Land use Sub-Division</label>
			</code>

			<boolean id="10" name="land_use_subdivision_accuracy"
				relevant="land_use_category!='noData'">
				<label xml:lang="en">Land Use Sub-Division interpretation accuracy</label>
			</boolean>

			<text name="site_directions" type="memo"  id="13" required="false" relevant="false()">
				<label xml:lang="en">Directions</label>
			</text>
			<entity id="14" name="topography" multiple="true">
				<label xml:lang="en">Topography</label>
				<code id="15" name="element" list="topographical_elements" key="true">
					<label xml:lang="en">Element</label>
				</code>
				<code id="16" name="coverage" list="topographical_elements_coverage">
					<label xml:lang="en">Coverage</label>
				</code>
			</entity>
			<date id="20" name="rs_date" relevant="land_use_category!='noData'">
				<label xml:lang="en">RS Date</label>
			</date>
			<code id="21" name="rs_satellite" list="rs_satellite"
				relevant="land_use_category!='noData'">
				<label xml:lang="en">RS Satellite</label>
			</code>
			<number id="25" name="elevation" type="integer">
				<label xml:lang="en">Elevation</label>
			</number>
			<number id="26" name="aspect" type="real">
				<label xml:lang="en">Aspect</label>
			</number>
			<number id="27" name="slope" type="real">
				<label xml:lang="en">Slope</label>
			</number>
			<boolean id="28" name="actively_saved">
				<label xml:lang="en">Actively saved by user</label>
			</boolean>
			<date id="1032" name="actively_saved_on" required="false" relevant="false()">
				<label xml:lang="en">Last saved on</label>
			</date>
			<text id="1034" name="plot_file" required="false" relevant="false()">
				<label xml:lang="en">CSV file that conatins the plot</label>
			</text>
			<text id="1038" name="region" required="false" relevant="false()">
				<label xml:lang="en">Region</label>
			</text>
		</entity>
	</schema>
